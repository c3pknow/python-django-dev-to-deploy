{
  "python.pythonPath": "./btre-real-estate-site/venv/bin/python",
  "files.exclude": {
    "__error": "Not found",
    "python_sandbox_starter": true,
    "*.DS_Store": true,
    "*.DS_Store.*": true,
    "*.git": true,
    "*.git.*": true,
    "*python_sandbox_finished": true,
    "*python_sandbox_finished.*": true,
    "*python_sandbox_starter": true,
    "*python_sandbox_starter.*": true
  },
  "python.linting.pylintArgs": ["--load-plugins=pylint_django"],
  "files.associations": {
    "**/templates/*.html": "django-html",
    "**/templates/*": "django-txt",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements"
  },
  "emmet.includeLanguages": {"django-html": "html"}
}